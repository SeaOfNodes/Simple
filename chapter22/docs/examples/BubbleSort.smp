// -*- mode: java;  -*-


val _exit = { u8[~] msg ->
    sys.io.p(msg);
    sys.libc._exit(1);
};

struct Scan {
    int !x;
    u8[~] buf;
};


// Read from stdin a line of the form "[ 4, 3, 2, 1]" and print it sorted.
val main = { ->
    Scan !s = new Scan{ buf = sys.io.stdin(); };
    var junk = new u8[1];
    while( s.x < s.buf# ) {
        sys.io.p("scan ");
        junk[0] = s.buf[s.x++];
        sys.io.p(junk);
    };
    return 0;
};
